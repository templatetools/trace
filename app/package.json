{
  "name": "wx_timekeeper",
  "version": "1.0.0",
  "description": "工时填报",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run buildDevVendor && npm run buildDev && npm run server",
    "buildDisVendor": "webpack --progress --env='dis' --config webpack.dll.js",
    "buildDevVendor": "webpack --progress --env='dev' --config webpack.dll.js",
    "buildDev": "webpack --progress --env='dev' --config webpack.config.babel.js",
    "buildDis": "webpack --progress --env='dis' --config webpack.config.babel.js",
    "build": "rm -rf dist && npm run buildDisVendor && npm run buildDis",
    "server": "babel-node server.js"
  },
  "author": "zhongfan@ruijie.com.cn",
  "license": "ISC",
  "dependencies": {
    "echarts": "^3.6.2",
    "prop-types": "^15.5.10",
