    "react-cookie": "^1.0.4",
    "react-dom": "^15.4.4",
    "react-router-dom": "^4.1.1",
    "react-touch-events": "^2.1.0",
    "react-weui": "^1.1.2",
    "weui": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
